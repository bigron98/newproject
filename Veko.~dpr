program Veko;

uses
  Forms,
  F_Depot in 'F_Depot.pas' {Depot: TDataModule},
  F_Desk_Top in 'F_Desk_Top.pas' {Desk_Top},
  F_Spr_Supplier in 'F_Spr_Supplier.pas' {Spr_Supplier},
  F_Ostatki in 'F_Ostatki.pas' {Ostatki},
  F_Global_Var in 'F_Global_Var.pas',
  F_Spr_Edizm in 'F_Spr_Edizm.pas' {Spr_Edizm},
  F_Spr_Config in 'F_Spr_Config.pas' {Spr_Config},
  F_Pages_Items in 'F_Pages_Items.pas' {Pages_Items},
  F_Prihod in 'F_Prihod.pas' {Prihod},
  F_Spr_Items in 'F_Spr_Items.pas' {Spr_Items},
  F_Preview in 'F_Preview.pas' {Preview},
  F_Login in 'F_Login.pas' {Login},
  F_Rashod_Date in 'F_Rashod_Date.pas' {Rashod_Date},
  F_Rashod in 'F_Rashod.pas' {Rashod};

{$R *.RES}

begin
  Application.Initialize;
  Application.CreateForm(TDepot, Depot);
  Application.CreateForm(TLogin, Login);
  Application.CreateForm(TDesk_Top, Desk_Top);
  Application.CreateForm(TSpr_Supplier, Spr_Supplier);
  Application.CreateForm(TOstatki, Ostatki);
  Application.CreateForm(TSpr_Edizm, Spr_Edizm);
  Application.CreateForm(TSpr_Config, Spr_Config);
  Application.CreateForm(TPages_Items, Pages_Items);
  Application.CreateForm(TPrihod, Prihod);
  Application.CreateForm(TSpr_Items, Spr_Items);
  Application.CreateForm(TPreview, Preview);
  Application.CreateForm(TRashod_Date, Rashod_Date);
  Application.CreateForm(TRashod, Rashod);
  Application.Run;
end.
